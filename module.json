{
  "name": "OBS Websocket",
  "type": "WebSocket Client",
  "path": "Software",
  "version": "2.0.6",
  "description": "To control OBS by Websocket. Module compatible with OBS Websocket 5 or more (directly integrated in OBS 28 or more).  To know all requests and parameters, see this link :https://github.com/obsproject/obs-websocket/blob/master/docs/generated/protocol.md#requests ",
  "url": "https://github.com/Edrig/OBS-Websocket-Chataigne-Module",
  "downloadURL": "https://github.com/Edrig/OBS-Websocket-Chataigne-Module/archive/master.zip",
  "hasInput": true,
  "hasOutput": true,
  "hideDefaultCommands": true,
  "hideDefaultParameters": [
    "autoAdd",
    "protocol",
    "messageStructure",
    "firstValueIsTheName",
    "useSecureConnection",
    "connected"
  ],
  "defaults": {
    "Protocol": "Lines",
    "autoAdd": false,
    "serverPath": "127.0.0.1:4455"
  },
  "parameters": {
    "Enable Authentification": {
      "type": "Boolean",
      "default": true
    },
    "Password": {
      "type": "String",
      "dependency": {
        "source": "Enable Authentification",
        "value": true,
        "check": "equals",
        "action": "show"
      }
    },
    "EventSub": {
      "type": "Container",
      "General": {
        "type": "Boolean",
        "default": true
      },
      "Config": {
        "type": "Boolean",
        "default": false
      },
      "Scenes": {
        "type": "Boolean",
        "default": true
      },
      "Inputs": {
        "type": "Boolean",
        "default": false
      },
      "Transitions": {
        "type": "Boolean",
        "default": false
      },
      "Filters": {
        "type": "Boolean",
        "default": false
      },
      "Outputs": {
        "type": "Boolean",
        "default": false
      },
      "SceneItems": {
        "type": "Boolean",
        "default": true
      },
      "MediaInputs": {
        "type": "Boolean",
        "default": false
      },
      "Vendors": {
        "type": "Boolean",
        "default": false
      },
      "Ui": {
        "type": "Boolean",
        "default": true
      },
      "InputVolumeMeters": {
        "type": "Boolean",
        "default": false
      },
      "InputActiveStateChanged": {
        "type": "Boolean",
        "default": false
      },
      "InputShowStateChanged": {
        "type": "Boolean",
        "default": false
      },
      "SceneItemTransformChanged": {
        "type": "Boolean",
        "default": true
      }
    },
    "See all requests of OBS": {
      "type": "String",
      "default": "https://github.com/obsproject/obs-websocket/blob/master/docs/generated/protocol.md#requests "
    }
  },
  "values": {
    "removeAllValues": {
      "type": "Trigger",
      "callback": "removeAllValues"
    },
    "ControlsStatus": {
      "type": "Container",
      "ControllerStatus": {
        "type": "Boolean",
        "description": "false = scenes, true = items",
        "default": false
      },
      "CropBotTopStatus": {
        "type": "Boolean",
        "description": "false = top, true = bot",
        "default": false
      },
      "CropLeftRightStatus": {
        "type": "Boolean",
        "description": "false = left, true = right",
        "default": false
      },
      "StudioModeStatus": {
        "type": "Boolean",
        "description": "OBS studio mode status; false = unactive, true = active",
        "default": false
      }
    },
    "Active Item Transform": {
      "type": "Container",
      "X": {
        "type": "Integer",
        "min": -9999,
        "max": 9999,
        "description": "X position"
      },
      "Y": {
        "type": "Integer",
        "min": -9999,
        "max": 9999,
        "description": "Y position"
      },
      "Zoom": {
        "type": "Float",
        "min": -5,
        "max": 5,
        "description": "Zoom in multiplicator"
      },
      "Rotation": {
        "type": "Integer",
        "min": -9999,
        "max": 9999,
        "description": "In degree"
      },
      "Crop_Left": {
        "type": "Integer",
        "min": -9999,
        "max": 9999
      },
      "Crop_Right": {
        "type": "Integer",
        "min": -9999,
        "max": 9999
      },
      "Crop_Top": {
        "type": "Integer",
        "min": -9999,
        "max": 9999
      },
      "Crop_Bot": {
        "type": "Integer",
        "min": -9999,
        "max": 9999
      },
      "Steppers": {
        "type": "Container",
        "X/Y/Crop/Rot Step": {
          "type": "Integer",
          "min": -9999,
          "max": 9999,
          "description": "Precision step",
          "default": 1
        },
        "Zoom Step": {
          "type": "Float",
          "min": -5,
          "max": 5,
          "description": "Precision step",
          "default": 0.001
        }
      }
    }
  },
  "scripts": [
    "OBS.js"
  ],
